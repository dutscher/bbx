{
  "name": "bbx-watcher",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "npm:install": "npm install",
    "buildNdeploy": "npm run build -- --prod && npm run deploy",
    "buildFastNDeploy": "npm run build:copy -- -- --prod && npm run deploy",
    "--- dev": "this is a divider",
    "dev": "npm run build:copy && rollup -c -w",
    "start-by-rollup": "sirv public --no-clear",
    "validate": "svelte-check",
    "--- update": "this is a divider",
    "fetch": "npm run exec ./scripts/fetch-stats.js",
    "fetch:compare": "npm run fetch -- -- --create-compare",
    "fetch:parse-pages": "npm run fetch -- -- --parse-pages --create-compare",
    "fetch:parse-pages-n-parts": "npm run fetch -- -- --parse-pages-n-parts --create-compare",
    "cat-n-tags": "npm run exec ./scripts/cats-n-tags.js",
    "parse-parts": "npm run exec ./scripts/parts-parse.js",
    "--- deploy": "this is a divider",
    "build": "rollup -c && npm run build:copy -- --prod",
    "build:movie-names": "npm run exec ./scripts/movie-names.js",
    "build:inst": "npm run exec ./scripts/inst-parse.js",
    "build:copy": "npm run exec ./scripts/build.js",
    "deploy": "npm run exec ./scripts/deploy.js",
    "exec": "node --experimental-json-modules --no-warnings"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "cheerio": "^1.0.0-rc.10",
    "filehound": "^1.17.4",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "jsdom": "^16.6.0",
    "modularscale-sass": "^3.0.10",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "node-sass": "^6.0.0",
    "request": "^2.88.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-svg": "^1.0.0-beta.2",
    "rollup-plugin-terser": "^7.0.0",
    "sanitize-html": "^2.4.0",
    "sharp": "^0.28.3",
    "ssh2-sftp-client": "^7.0.0",
    "ssl-root-cas": "^1.3.1",
    "svelte": "^3.0.0",
    "svelte-apollo": "^0.4.0",
    "svelte-assets-preprocessor": "^0.0.7",
    "svelte-check": "^1.0.0",
    "svelte-click-outside": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "svelte-watch-resize": "^1.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.0.0",
    "sirv-cli": "^1.0.0",
    "yargs": "^17.3.0"
  }
}
